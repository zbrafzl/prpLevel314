}

@model Prp.Sln.ApplicantApplicationModel
@section RenderCss{

}

@section RenderScript{

    <script>
        var applicantId = 0;
        $(document).ready(function () {

        });

    </script>
}
<div class="container content">
    <div class="row main">
        @Html.Partial("_ApplicantLinks")
        <div class="col-sm-12 cv-box">
            <div class="mt10">
                <p>Thank you for completing Profile Wizard with PRP Portal.Please generate and download the fee voucher to proceed. </p>
