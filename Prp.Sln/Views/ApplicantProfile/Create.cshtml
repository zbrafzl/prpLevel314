
@{
    ViewBag.Title = "Profile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model PrpSln.ProfileModel
@section RenderCss{

}
@section RenderScript{
    <!-- jQuery Smart Wizard -->
    @*<script src="/vendors/jQuery-Smart-Wizard/js/jquery.smartWizard.js"></script>*@
    <script>

        var action = "PersonalInfo", step = 1, provinceIdInfo = 0, districtIdInfo = 0;
        var applicantDetailId = 0, applicantDegreeDetailId = 0, specialityIdFCPS = 0, specialityIdMSMD = 0, imagesPath = "/Images/Applicant/";

        var graduateType = 1, htmlTempExp = "", htmlTempDisnt = "";

        var objApplication = {}, objApplicant = {}, objApplicantInfo = {}, objDegree = {}, objEducation = {},
            objDegreeMarksList = [], objSpecialityList = [], objExperience = {}, objExperienceList = [];

        $(document).ready(function () {

            imagesPath = imagesPath + applicantId + "/"
            $("#step1").addClass("selected");
            $("#step1").removeClass("disabled");
            step = 3;

            $(".grd-foreign").hide();

            htmlTempExp = $("#tbBodyExp").html();
            htmlTempDisnt = $("#tbBodyDisnt").html();

            MoveWizardPanel(step);

            BindInstituteDDL(1, 0);
            BindAllPanelsData();

            $('#txtDob').datetimepicker({
                format: 'DD/MM/YYYY',
                maxDate: new Date()
            });

            $('#txtExpiryDatePMDC').datetimepicker({
                format: 'DD/MM/YYYY',
                minDate: new Date()
            });

            $('#txtDateOfPassingMBBS').datetimepicker({
                format: 'DD/MM/YYYY',
                maxDate: new Date()
            });

            $('#txtCNICExpiryDate').datetimepicker({
                format: 'DD/MM/YYYY',
                minDate: new Date()
            });

            $('#txtPassingDateFCPS').datetimepicker({
                format: 'DD/MM/YYYY',
                maxDate: new Date()
            });

            $('#txtPassingDateMSMD').datetimepicker({
                format: 'DD/MM/YYYY',
                maxDate: new Date()
            });

            $('#txtExpDateStart').datetimepicker({
                format: 'DD/MM/YYYY',
                maxDate: new Date()
            });

            $('#txtExpDateEnd').datetimepicker({
                format: 'DD/MM/YYYY',
                maxDate: new Date()
            });

            // For Pakistan = 132
            $('#ddlCountry').val(132);

            //step-wizard

            $(document).on('change', '#ddlProvince', function () {
                var provinceId = $(this).val();

                DDLSetToDefault("ddlDistrict", 'Select One', 0);
                $("#hfdDistrictId").val(0);

                if (provinceId > 0)
                    BindDistrictDDL(provinceId, 0);
            });

            $(document).on('click', '.step-head', function () {

                step = ConvertToInt(step);
                var attrId = $(this).attr("id");
                var id = attrId.replace("step", "");

                var hasClass = $(this).hasClass("disabled")
                if (hasClass == true) {
                    return;
                }
                if (step == id) {
                    return;
                }
                step = id;

                MoveWizardPanel(step);

            });

            $(document).on('change', '.flup', function () {

                var idAttr = $(this).attr("id");

                var ctrlImg = idAttr.replace("fl", "img");
                //imgCnicFront
                var _size = this.files[0].size;
                var fSExt = new Array('Bytes', 'KB', 'MB', 'GB'),
                    i = 0; while (_size > 900) { _size /= 1024; i++; }

                var exactSize = (Math.round(_size * 100) / 100);
                if (exactSize > 500) {
                    var exactSizeFull = (Math.round(_size * 100) / 100) + ' ' + fSExt[i];
                    alert("File size " + exactSizeFull + " greater then 500 KB. Please compress image size.");
                    $(this).val("");
                    $('#' + ctrlImg).attr('src', "");
                    $('#' + ctrlImg).hide();
                }
                else {
                    readURL(this, ctrlImg);
                }
            });


            //

            //  Satrt Education Tab

            //

            $(document).on('change', '#ddlInstituteType', function () {
                var instituteType = $(this).val();

                DDLSetToDefault("ddlInstitute", 'Select One', 0);


                if (instituteType > 0)
                    BindInstituteDDL(instituteType, 0);
            });

            $(document).on('change', 'input:radio[name="rbtnGraduate"]', function () {

                var attrId = $(this).attr("id");
                if (attrId == "rbtnGraduateLocal") {
                    $(".grd-foreign").hide();
                    $(".grd-local").show();
                    graduateType = 1;
                }
                else if (attrId == "rbtnGraduateForeign") {

                    $(".grd-local").hide();
                    $(".grd-foreign").show();
                    graduateType = 2;
                }
            });

            $(document).on('change', 'input:radio[name="rbtnHouseJob"]', function () {

                var attrId = $(this).attr("id");
                if (attrId == "rbtnHouseJobYes") {
                    $(".divHouseJob").show();
                }
                else if (attrId == "rbtnHouseJobNo") {

                    $(".divHouseJob").hide();
                }
            });

            //marks

            $(document).on('keyup', '.marksd', function () {

                var attrId = $(this).attr("id");
                var attrId2nd = "";
                var obtainMarks = 0, totalMarks = 0;
                if (attrId.toLowerCase().indexOf("obtain") !== -1) {

                    attrId2nd = attrId.replace("Obtain", "Total");
                    obtainMarks = ConvertToInt($(this).val());
                    totalMarks = ConvertToInt($("#" + attrId2nd).val());
                    if (obtainMarks > 0) {
                        if (totalMarks > 0 && obtainMarks > totalMarks) {

                            alert("Please enter obtain marks less then total marks.");
                        }
                    }

                }
                else if (attrId.toLowerCase().indexOf("total") !== -1) {

                    attrId2nd = attrId.replace("Total", "Obtain");
                    totalMarks = ConvertToInt($(this).val());
                    obtainMarks = ConvertToInt($("#" + attrId2nd).val());
                    if (totalMarks > 0) {
                        if (obtainMarks > 0 && totalMarks < obtainMarks) {

                            alert("Please enter total marks greater then obtain marks.");
                        }
                    }
                }


            });

            $(document).on('click', '#chkFCPS', function () {
                var status = $("#chkFCPS").is(':checked');
                $("#divFCPS").hide();
                if (status == true) {
                    $("#divFCPS").show();
                }
            });

            $(document).on('click', '#chkMSMD', function () {
                var status = $("#chkMSMD").is(':checked');
                $("#divMSMD").hide();
                if (status == true) {
                    $("#divMSMD").show();
                }
            });

            //  End Education Tab

            // Exaprience Tab Start

            $(document).on('change', '#ddlExpInstitueLevel', function () {
                var instituteLevel = $(this).val();

                DDLSetToDefault("ddlExpLevelType", 'Select One', 0);


                if (instituteLevel > 0)
                    BindLevelTypeDDL(instituteLevel, 0);
            });

            //Exaprience Tab End

            $(document).on('click', '#btnExpSave', function () {

                SaveExperienceDate();
            });

            $(document).on('click', '#btnSaveAndProceed', function () {

                step = ConvertToInt(step);
                if (step == 1) {

                    objApplicantInfo = ValidateFormInfo();

                    if (objApplicantInfo.isError == false) {

                        var resp = CallActionPost("/ApplicantProfile/ApplicantInfoAddUpdate", objApplicantInfo);
                        if (resp != null && resp.status == true) {

                            var respInfo = CallActionGet("/ApplicantProfile/GetApplicantInfoData?applicantId=" + applicantId);
                            if (respInfo != null) {

                                if (respInfo != null && respInfo.applicantDetailId > 0) {

                                    objApplicantInfo = respInfo;
                                    BindPenelInfo(false);
                                }
                            }
                            step = step + 1;
                            MoveWizardPanel(step);
                            $("#btnPrivous").show();

                        }
                    }
                }
                else if (step == 2) {


                    if (ValidateFormEducation() == false) {

                        var resp = CallActionPost("/ApplicantProfile/ApplicantEducationAddUpdate", objEducation);
                        if (resp != null && resp.status == true) {

                            var respEdu = CallActionGet("/ApplicantProfile/GetApplicantEducationData?applicantId=" + applicantId);
                            if (respEdu != null) {

                                if (respEdu.applicantDegree != null && respEdu.applicantDegree.applicantDegreeDetailId > 0) {
                                    objDegree = respEdu.applicantDegree;
                                    //BindPenelInfo(false);
                                }

                                if (respEdu.listDegreeMarks != null && respEdu.listDegreeMarks.length > 0) {
                                    objDegreeMarksList = respEdu.listDegreeMarks;
                                    //BindPenelInfo(false);
                                }

                                if (respEdu.listSpeciality != null && respEdu.listSpeciality.length > 0) {
                                    objSpecialityList = respEdu.listSpeciality;
                                    //BindPenelInfo(false);
                                }
                            }
                            step = step + 1;
                            MoveWizardPanel(step);
                            $("#btnPrivous").show();

                        }
                    }
                }
            });

            $(document).on('click', '#btnPrivous', function () {

                step = ConvertToInt(step);
                step = step - 1;
                MoveWizardPanel(step);


                $("#btnPrivous").hide();

                if (step > 1) {
                    $("#btnPrivous").show();
                }
            });





        });

        function BindDistrictDDL(provinceId, districtId) {

            var list = CallActionGet("/Common/RegionGetByCondition?typeId=4" + "&parentId=" + provinceId + "&condition=");
            if (list != null && list.length > 0) {

                var ddlItem = {};
                for (var i = 0; i < list.length; i++) {
                    ddlItem = list[i];

                    if (districtId == ddlItem.regionId)
                        $("#ddlDistrict").append("<option  value='" + ddlItem.regionId + "' selected='selected'>" + ddlItem.name + "</option>");
                    else $("#ddlDistrict").append("<option  value='" + ddlItem.regionId + "'>" + ddlItem.name + "</option>");
                }
            }

        }

        function BindInstituteDDL(instituteTypeId, instituteId) {

            var list = CallActionGet("/Common/InstitueGetByType?typeId=" + instituteTypeId);
            if (list != null && list.length > 0) {

                var ddlItem = {};
                for (var i = 0; i < list.length; i++) {
                    ddlItem = list[i];

                    if (instituteId == ddlItem.instituteId)
                        $("#ddlInstitute").append("<option  value='" + ddlItem.instituteId + "' selected='selected'>" + ddlItem.name + "</option>");
                    else $("#ddlInstitute").append("<option  value='" + ddlItem.instituteId + "'>" + ddlItem.name + "</option>");
                }
            }

        }

        function BindLevelTypeDDL(instituteLevel, levelId) {

            var list = CallActionGet("/Common/ConstantGetByType?typeId=" + instituteLevel);
            if (list != null && list.length > 0) {

                var ddlItem = {};
                for (var i = 0; i < list.length; i++) {
                    ddlItem = list[i];

                    if (levelId == ddlItem.id)
                        $("#ddlExpLevelType").append("<option  value='" + ddlItem.id + "' selected='selected'>" + ddlItem.name + "</option>");
                    else $("#ddlExpLevelType").append("<option  value='" + ddlItem.id + "'>" + ddlItem.name + "</option>");
                }
            }
        }

        function readURL(input, ctrlImg) {
            if (input.files && input.files[0]) {
                var reader = new FileReader();

                reader.onload = function (e) {
                    $('#' + ctrlImg).attr('src', e.target.result);
                    $('#' + ctrlImg).show();
                }
                reader.readAsDataURL(input.files[0]);
            }
        }

        function MoveWizardPanel(id) {

            $(".step-head").removeClass("selected");
            $(".step-head").removeClass("disabled");
            $(".step-head").addClass("disabled");
            $(".step-head").removeClass("done");
            $(".step-body").hide();

            if (id > 1) {
                for (var i = 1; i < id; i++) {
                    $("#step" + i).removeClass("selected");
                    $("#step" + i).removeClass("disabled");
                    $("#step" + i).addClass("done");
                }
            }


            $("#step" + id).removeClass("disabled");
            $("#step" + id).addClass("selected");

            $("#step-" + id).show();
        }

        function SaveExperienceDate() {

        }

        function ValidateFormInfo() {


            var isError = false, ctrl = "";
            objApplicantInfo = {};

            var genderId = 0, isDisable = 0;

            if ($("#rbtnGenderMale").is(':checked'))
                genderId = 1;
            else if ($("#rbtnGenderFemale").is(':checked'))
                genderId = 2;

            if ($("#rbtnDisableYes").is(':checked'))
                isDisable = true;
            else if ($("#rbtnDisableNo").is(':checked'))
                isDisable = false;

            objApplicantInfo.fatherName = ConvertToString($("#txtFatherName").val());
            objApplicantInfo.genderId = genderId;
            objApplicantInfo.isDisable = isDisable;
