        function BindAllPanelsData() {


            var resp = CallActionGet("/ApplicantProfile/GetApplicationDetailData?applicantId=" + applicantId);
            if (resp != null) {

                if (resp.applicantInfo != null && resp.applicantInfo.applicantDetailId > 0) {

                    objApplicant = resp.applicant;
                    objApplicantInfo = resp.applicantInfo;
                    BindPenelInfo(true);
                }


                if (resp.applicantDegree != null && resp.applicantDegree.applicantDegreeDetailId > 0) {
                    objDegree = resp.applicantDegree;
                    objDegreeMarksList = resp.listDegreeMarks;
                    objSpecialityList = resp.listSpeciality;

                    BindPenelEducation(true);
                }


            }

        }

        function BindPenelInfo(IsRefresh) {

            applicantDetailId = objApplicantInfo.applicantDetailId;

            $("#txtFatherName").val(objApplicantInfo.fatherName);

            var dateTicks = parseInt(objApplicantInfo.dob.TooOnlyNumbers());

            $('#txtDob').datetimepicker({
                date: new Date(dateTicks),
                format: 'DD/MM/YYYY',
                maxDate: new Date()
            });

            dateTicks = parseInt(objApplicantInfo.pmdcExpiryDate.TooOnlyNumbers());
            $('#txtExpiryDatePMDC').datetimepicker({
                date: new Date(dateTicks),
                format: 'DD/MM/YYYY',
                minDate: new Date()
            });

            dateTicks = parseInt(objApplicantInfo.mbbsPassingDate.TooOnlyNumbers());
            $('#txtDateOfPassingMBBS').datetimepicker({
                date: new Date(dateTicks),
                format: 'DD/MM/YYYY',
                maxDate: new Date()
            });


            $("#ddlCountry").val(objApplicantInfo.countryId);
            $("#ddlProvince").val(objApplicantInfo.provinceId);
            if (IsRefresh == true)
                BindDistrictDDL(objApplicantInfo.provinceId, objApplicantInfo.districtId);
            else
                $("#ddlDistrict").val(objApplicantInfo.districtId);

            $("#ddlDomicileProvince").val(objApplicantInfo.domicileProvinceId);
            $("#txtAddress").val(objApplicantInfo.address);
            $("#txtCNIC").val(objApplicantInfo.cnicNo);

            dateTicks = parseInt(objApplicantInfo.cnicExpiryDate.TooOnlyNumbers());
            $('#txtCNICExpiryDate').datetimepicker({
                date: new Date(dateTicks),
                format: 'DD/MM/YYYY',
                minDate: new Date()
            });

            $('#imgCnicFront').attr('src', imagesPath + objApplicantInfo.cnicPicFront);
            $('#imgCnicFront').show();

            $('#imgCnicBack').attr('src', imagesPath + objApplicantInfo.cnicPicBack);
            $('#imgCnicBack').show();

            $('#imgDomicileFront').attr('src', imagesPath + objApplicantInfo.domicilePicFront);
            $('#imgDomicileFront').show();

            $('#imgDomicileBack').attr('src', imagesPath + objApplicantInfo.domicilePicBack);
            $('#imgDomicileBack').show();

            $('#imgPic').attr('src', imagesPath + objApplicant.pic);
            $('#imgPic').show();


            $("#hfdCnicFront").val(objApplicantInfo.cnicPicFront);
            $("#hfdCnicBack").val(objApplicantInfo.cnicPicBack);
            $("#hfdPic").val(objApplicant.pic);
            $("#hfdDomicileFront").val(objApplicantInfo.domicilePicFront);
            $("#hfdDomicileBack").val(objApplicantInfo.domicilePicBack);


            if (objApplicantInfo.genderId == 1)
                $("#rbtnGenderMale").attr("checked", "checked");
            else
                $("#rbtnGenderFemale").attr("checked", "checked");

            if (objApplicantInfo.isDisable == true)
                $("#rbtnDisableYes").attr("checked", "checked");
            else
                $("#rbtnDisableNo").attr("checked", "checked");
        }

        function BindPenelEducation(IsRefresh) {

            applicantDegreeDetailId = objDegree.applicantDegreeDetailId;

            $("#ddlDegree").val(objDegree.degreeTypeId);
            $("#ddlNoOfYear").val(objDegree.degreeYear);
            $("#ddlInstituteType").val(objDegree.instituteTypeId);
            if (IsRefresh == true)
                BindInstituteDDL(objDegree.instituteTypeId, objDegree.instituteId);
            else
                $("#ddlInstitute").val(objDegree.instituteId);

            $("#hfdDegree").val(objDegree.imageDegree);
            if (objDegree.imageDegree != "") {
                $('#imgDegree').attr('src', imagesPath + objDegree.imageDegree);
                $('#imgDegree').show();
            }


            if (objDegree.graduateTypeId == 1) {

                $(".grd-local").show();
                $(".grd-foreign").hide();

                $("#rbtnGraduateLocal").attr("checked", "checked");

                for (var i = 1; i <= objDegreeMarksList.length; i++) {

                    var objDegreeMarks = objDegreeMarksList[i - 1];

                    $("#hfdRowDegree" + i + "Id").val(objDegreeMarks.degreeMarksId);
                    $("#txtRowDegree" + i + "MarksObtain").val(objDegreeMarks.obtainMarks);
                    $("#txtRowDegree" + i + "MarksTotal").val(objDegreeMarks.totalMarks);
                    $("#ddlRowDegree" + i + "Attempt").val(objDegreeMarks.attempt);


                    $("#hfdRowDegree" + i).val(objDegreeMarks.imageDMC);
                    if (objDegreeMarks.imageDMC != "") {
                        $("#imgRowDegree" + i).attr('src', imagesPath + objDegreeMarks.imageDMC);
                        $("#imgRowDegree" + i).show();
                    }

                }
            }
            else if (objDegree.graduateTypeId == 2) {

                $(".grd-local").hide();
                $(".grd-foreign").show();

                $("#rbtnGraduateForeign").attr("checked", "checked");


                for (var i = 1; i <= objDegreeMarksList.length; i++) {

                    var objDegreeMarks = objDegreeMarksList[i];

                    $("#hfdRowPMDC" + i + "Id").val(objDegreeMarks.degreeMarksId);

                    $("#ddlRowPMDC" + i + "Attempt").val(objDegreeMarks.attempt);


                    $("#hfdRowPMDC" + i).val(objDegreeMarks.imageDMC);
                    if (objDegreeMarks.imageDMC != "") {
                        $("#imgRowPMDC" + i).attr('src', imagesPath + objDegreeMarks.imageDMC);
                        $("#imgRowPMDC" + i).show();
                    }

                }


                $("#txtTotalMarksForeign").val(objDegree.totalMarks);
                $("#txtObtainMarksForeign").val(objDegree.obtainMarks);

                $("#hfdDMCForeignFront").val(objDegree.imageDegreeForeignFront);
                if (objDegree.imageDegreeForeignFront != "") {

                    $('#imgDMCForeignFront').attr('src', imagesPath + objDegree.imageDegreeForeignFront);
                    $('#imgDMCForeignFront').show();
                }

                $("#hfdDMCForeignBack").val(objDegree.imageDegreeForeignBack);
                if (objDegree.imageDegreeForeignBack != "") {

                    $('#imgDMCForeignBack').attr('src', imagesPath + objDegree.imageDegreeForeignBack);
                    $('#imgDMCForeignBack').show();
                }

            }


            if (objDegree.isHouseJob == true) {
                $(".divHouseJob").show();
                $("#rbtnHouseJobYes").attr("checked", "checked");
                $("#ddlInstituteHouseJob").val(objDegree.instituteIdHouseJob);
                $("#ddlProvinceHouseJob").val(objDegree.provinceIdHouseJob);
                $("#hfdHouseJobCertificate").val(objDegree.imageHouseJob);
                if (objDegree.imageHouseJob != "") {

                    $('#imgHouseJobCertificate').attr('src', imagesPath + objDegree.imageHouseJob);
                    $('#imgHouseJobCertificate').show();
                }
            }
            else {
                $(".divHouseJob").hide();
                $("#rbtnHouseJobNo").attr("checked", "checked");
            }

            if (objDegree.isFCPS == true) {
                $("#divFCPS").show();


                if (objSpecialityList != null && objSpecialityList.length > 0) {
                    var objSpeciality = $.grep(objSpecialityList, function (n, i) {
                        return n.specialityTypeId == 1;
                    })[0];

                    if (objSpeciality != null) {

                        $("#chkFCPS").attr("checked", "checked");
                        $("#hfdFCPS").val(objSpeciality.specialityId);
                        $("#txtObtainMarksFCPS").val(objSpeciality.obtainMarks);
                        $("#txtTotalMarksFCPS").val(objSpeciality.totalMarks);

                        var dateTicks = parseInt(objSpeciality.degreeDate.TooOnlyNumbers());

                        $('#txtPassingDateFCPS').datetimepicker({
                            date: new Date(dateTicks),
                            format: 'DD/MM/YYYY',
                            maxDate: new Date()
                        });

                        $("#hfdDMCFCPS").val(objSpeciality.imageCertificate);
                        if (objSpeciality.imageCertificate != "") {

                            $('#imgDMCFCPS').attr('src', imagesPath + objSpeciality.imageCertificate);
                            $('#imgDMCFCPS').show();
                        }
                    }
                }

            }
            else {
                $("#divFCPS").hide();
                $("#chkFCPS").attr("checked", "");
            }

            if (objDegree.isMSMD == true) {

                $("#divMSMD").show();

                $("#chkMSMD").attr("checked", "checked");
                if (objSpecialityList != null && objSpecialityList.length > 0) {

                    var objSpeciality = $.grep(objSpecialityList, function (n, i) {
                        return n.specialityTypeId == 2;
                    })[0];

                    if (objSpeciality != null) {

                        $("#hfdMSMD").val(objSpeciality.specialityId);
                        $("#txtObtainMarksMSMD").val(objSpeciality.obtainMarks);
                        $("#txtTotalMarksMSMD").val(objSpeciality.totalMarks);

                        var dateTicks = parseInt(objSpeciality.degreeDate.TooOnlyNumbers());

                        $('#txtPassingDateMSMD').datetimepicker({
                            date: new Date(dateTicks),
                            format: 'DD/MM/YYYY',
                            maxDate: new Date()
                        });

                        $("#hfdDMCMSMD").val(objSpeciality.imageCertificate);
                        if (objSpeciality.imageCertificate != "") {

                            $('#imgDMCMSMD').attr('src', imagesPath + objSpeciality.imageCertificate);
                            $('#imgDMCMSMD').show();
                        }
                    }
                }

            }
            else {
                $("#divMSMD").hide();
                $("#chkMSMD").attr("checked", "");
            }

        }


