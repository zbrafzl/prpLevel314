        function ValidateFormEducation() {

            var isError = false, ctrl = "";
            objDegree = {}; objDegreeMarksList = []; objSpecialityList = [];

            objDegree.applicantDegreeDetailId = applicantDegreeDetailId;
            objDegree.applicantId = applicantId;

            if ($("#rbtnGraduateLocal").is(':checked'))
                objDegree.graduateTypeId = 1;
            else if ($("#rbtnGraduateForeign").is(':checked'))
                objDegree.graduateTypeId = 2;

            objDegree.degreeTypeId = ConvertToInt($("#ddlDegree").val());
            objDegree.degreeYear = ConvertToInt($("#ddlNoOfYear").val());
            objDegree.instituteTypeId = ConvertToInt($("#ddlInstituteType").val());
            objDegree.instituteId = ConvertToInt($("#ddlInstitute").val());

            if ($("#flDegree").val()) {
                objDegree.imageDegree = UploadImage("flDegree", applicantId, "Degree");
                $("#hfdDegree").val(objDegree.imageDegree);
            }
            objDegree.imageDegree = ConvertToString($("#hfdDegree").val());


            objDegreeMarksList = [];
            if (objDegree.graduateTypeId == 1) {


                for (var i = 1; i <= 5; i++) {

                    var objDegreeMarks = {};
                    objDegreeMarks.degreeMarksId = ConvertToInt($("#hfdRowDegree" + i + "Id").val());
                    objDegreeMarks.applicantId = applicantId;
                    objDegreeMarks.year = i;
                    objDegreeMarks.obtainMarks = ConvertToInt($("#txtRowDegree" + i + "MarksObtain").val());
                    objDegreeMarks.totalMarks = ConvertToInt($("#txtRowDegree" + i + "MarksTotal").val());
                    objDegreeMarks.attempt = ConvertToInt($("#ddlRowDegree" + i + "Attempt").val());

                    if ($("#flRowDegree" + i).val()) {
                        objDegreeMarks.imageDMC = UploadImage("flRowDegree" + i, applicantId, "DMCDegree" + i);
                        $("#hfdRowDegree" + i).val(objDegreeMarks.imageDMC);
                    }
                    objDegreeMarks.imageDMC = ConvertToString($("#hfdRowDegree" + i).val());


                    objDegreeMarksList.push(objDegreeMarks);
                }

                objDegree.totalMarks = 0;
                objDegree.obtainMarks = 0;
                objDegree.imageDegreeForeignFront = "";
                objDegree.imageDegreeForeignBack = "";
            }
            else if (objDegree.graduateTypeId == 2) {

                for (var i = 1; i <= 3; i++) {

                    var objDegreeMarks = {};
                    objDegreeMarks.degreeMarksId = ConvertToInt($("#txtRowPMDC" + i + "Id").val());
                    objDegreeMarks.applicantId = applicantId;
                    objDegreeMarks.year = i;
                    objDegreeMarks.obtainMarks = 0;
                    objDegreeMarks.totalMarks = 0;
                    objDegreeMarks.attempt = ConvertToInt($("#ddlRowPMDC" + i + "Attempt").val());

                    if ($("#flRowPMDC" + i).val()) {
                        objDegreeMarks.imageDMC = UploadImage("flRowPMDC" + i, applicantId, "DMCPMDC" + i);
                        $("#hfdRowPMDC" + i).val(objDegreeMarks.imageDMC);
                    }
                    objDegreeMarks.imageDMC = ConvertToString($("#hfdRowPMDC" + i).val());

                    objDegreeMarksList.push(objDegreeMarks);
                }

                objDegree.totalMarks = ConvertToInt($("#txtTotalMarksForeign").val());
                objDegree.obtainMarks = ConvertToInt($("#txtObtainMarksForeign").val());


                if ($("#flDMCForeignFront").val()) {
                    objDegree.imageDegreeForeignFront = UploadImage("flDMCForeignFront", applicantId, "DegreeDMCBack");
                    $("#hfdDMCForeignBack").val(objDegree.imageDegreeForeignFront);
                }
                objDegree.imageDegreeForeignFront = ConvertToString($("#hfdDMCForeignBack").val());

                if ($("#fllDMCForeignBack").val()) {
                    objDegree.imageDegreeForeignBack = UploadImage("fllDMCForeignBack", applicantId, "DegreeDMCBack");
                    $("#hfdDMCForeignBack").val(objDegree.imageDegreeForeignBack);
                }
                objDegree.imageDegreeForeignBack = ConvertToString($("#hfdDMCForeignBack").val());



            }

            // House Job
            if ($("#rbtnHouseJobYes").is(':checked'))
                objDegree.isHouseJob = true;
            else if ($("#rbtnHouseJobNo").is(':checked'))
                objDegree.isHouseJob = false;

            if (objDegree.isHouseJob == true) {

                objDegree.instituteIdHouseJob = ConvertToInt($("#ddlInstituteHouseJob").val());
                objDegree.provinceIdHouseJob = ConvertToInt($("#ddlProvinceHouseJob").val());

                if ($("#flHouseJobCertificate").val()) {
                    objDegree.imageHouseJob = UploadImage("flHouseJobCertificate", applicantId, "HouseJob");
                    $("#hfdHouseJobCertificate").val(objDegree.imageHouseJob);
                }
                objDegree.imageHouseJob = ConvertToString($("#hfdHouseJobCertificate").val());
            }

            //txtObtainMarksFCPS

            objDegree.isFCPS = $("#chkFCPS").is(':checked');

            if (objDegree.isFCPS == true) {

                var objSpeciality = {};
                objSpeciality.specialityId = ConvertToInt($("#hfdFCPS").val());
                objSpeciality.specialityTypeId = 1;
                objSpeciality.applicantId = applicantId;
                objSpeciality.obtainMarks = ConvertToInt($("#txtObtainMarksFCPS").val());
                objSpeciality.totalMarks = ConvertToInt($("#txtTotalMarksFCPS").val());
                objSpeciality.degreeDate = ConvertToString($("#txtPassingDateFCPS").data('date'));

                if ($("#flDMCFCPS").val()) {
                    objSpeciality.imageCertificate = UploadImage("flDMCFCPS", applicantId, "FCPS");
                    $("#hfdDMCFCPS").val(objSpeciality.imageCertificate);
                }
                objSpeciality.imageCertificate = ConvertToString($("#hfdDMCFCPS").val());

                objSpecialityList.push(objSpeciality);
            }

            objDegree.isMSMD = $("#chkMSMD").is(':checked');

            if (objDegree.isMSMD == true) {

                var objSpeciality = {};
                objSpeciality.specialityId = ConvertToInt($("#hfdMSMD").val());
                objSpeciality.specialityTypeId = 2;
                objSpeciality.applicantId = applicantId;
                objSpeciality.obtainMarks = ConvertToInt($("#txtObtainMarksMSMD").val());
                objSpeciality.totalMarks = ConvertToInt($("#txtTotalMarksMSMD").val());

                objSpeciality.degreeDate = ConvertToString($("#txtPassingDateMSMD").data('date'));

                if ($("#flDMCMSMD").val()) {
