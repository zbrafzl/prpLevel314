@{
    ViewBag.Title = "Home";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using Prp.Sln;

@model Prp.Sln.HomeModel
@section RenderCss{

    <style>
        .btnRegister {
            width: 14%;
            margin-top: 0px;
        }
    </style>

}
@section RenderScript{

    <script>
        $(document).ready(function () {

        });
    </script>
}
<div class="container">
    <div class="row">
        <div id="wrapper" style="position:inherit">
           
            <div class="col-md-12">
                <div id="linkMeritList" class="pnlMeritList" style="display:none;">
                    <h1>Merit List Round @ProjConstant.consentRound</h1>
                    <div class="col-sm-12 text-center">
                        <a href="@("/m-1/"+ ProjConstant.preUrl+"/1" )" target="_blank" class="btn btnRegister animated-button">  FCPS </a>
                        <a href="@( "/m-2/"+ProjConstant.preUrl+"/2" )" target="_blank" class="btn btnRegister animated-button">  MS </a>
                        <a href="@("/m-3/"+ ProjConstant.preUrl+"/3" )" target="_blank" class="btn btnRegister animated-button">  MD </a>
                        <a href="@( "/m-4/"+ProjConstant.preUrl+"/4")" target="_blank" class="btn btnRegister animated-button">  MDS </a>
                        <a href="@("/m-5/"+ ProjConstant.preUrl+"/5" )" target="_blank" class="btn btnRegister animated-button">  FCPS Dentistry </a>
                    </div>
                </div>
                <div>
                    <h1>Gazatte List</h1>
                    <div class="col-sm-12 text-center">
                        <a href="@("/g/"+ ProjConstant.preUrl+"/1" )" target="_blank" class="btn btnRegister animated-button">  FCPS </a>
                        <a href="@( "/g/"+ProjConstant.preUrl+"/2" )" target="_blank" class="btn btnRegister animated-button">  MS </a>
                        <a href="@("/g/"+ ProjConstant.preUrl+"/3" )" target="_blank" class="btn btnRegister animated-button">  MD </a>
                        <a href="@( "/g/"+ProjConstant.preUrl+"/4")" target="_blank" class="btn btnRegister animated-button">  MDS </a>
                        <a href="@("/g/"+ ProjConstant.preUrl+"/5" )" target="_blank" class="btn btnRegister animated-button">  FCPS Dentistry </a>
                    </div>
                </div>

                <div class="landingWarp">
                    <h1>Eligibility Criteria</h1>
                    <ul>
                        <li><i class="fa fa-check" aria-hidden="true"></i>MBBS/BDS Degree.</li>
                        <li><i class="fa fa-check" aria-hidden="true"></i>Certificate of Completion of 12 months of House Job.</li>
                        <li><i class="fa fa-check" aria-hidden="true"></i>PMDC Registration for Pakistani Graduates /License to practice by relevant bodies for foreign graduates.</li>
                        <li><i class="fa fa-check" aria-hidden="true"></i>Result of passing PART-I Examination of FCPS/MD/MS/MDS within last 03 years.</li>
                    </ul>
                    <div class="col-sm-12 text-center">
                        <a href="/term-and-condition" target="_blank" class="btn btnRegister animated-button"> Notifications </a>
                        <a href="/term-and-condition" target="_blank" class="btn btnRegister animated-button">POLICIY PG LEVEL-III </a>
                        <a href="/term-and-condition" target="_blank" class="btn btnRegister animated-button">User Manual </a>
                        <a href="/term-and-condition" target="_blank" class="btn btnRegister animated-button">Seats </a>
                        <a href="/register" class="btn btnRegister animated-button">Register </a>
                        <a href="/login" class="btn btnRegister animated-button">Login </a>
                    </div>
                    <div class="col-sm-12 text-center" style="margin-top:10px;">
                        <a href="http://level4.phf.gop.pk/" style="margin-left:30%; width:30% !important; background: #f25353 !important;" class="btn btnRegister animated-button">Apply to Level 4 Click Here </a>
                    </div>
                    <div class="col-sm-12 text-center" style="margin-top:10px;">
                        <a href="/login" style="margin-left:30%; width:30% !important; background: #6b88f2 !important;" class="btn btnRegister animated-button">Apply to Level 3 Click Here </a>
                    </div>
                    <h1>Seats</h1>
                    <div class="col-sm-12 text-center">
                        <a href="@("/seats/" + ProjConstant.Seat1)" target="_blank" class="btn btnRegister animated-button"> FCPS </a>
                        <a href="@("/seats/" + ProjConstant.Seat2)" target="_blank" class="btn btnRegister animated-button">MS </a>
                        <a href="@("/seats/" + ProjConstant.Seat3)" target="_blank" class="btn btnRegister animated-button">MD </a>
                        <a href="@("/seats/" + ProjConstant.Seat4)" target="_blank" class="btn btnRegister animated-button">MDS </a>
                        <a href="@("/seats/" + ProjConstant.Seat5)" target="_blank" class="btn btnRegister animated-button">FCPS-Dentistry </a>
                    </div>

                    <h1>Applicant Data</h1>
                    <div class="col-sm-12 text-center">
                        <a href="@("/app-list/" + ProjConstant.applicationStatusProfile)" target="_blank" class="btn btnRegister animated-button"> Profile </a>
                    </div>
                </div>
            </div>
            <div class="col-md-10 col-md-offset-1">
                <div style="margin-top: 10px;">
                </div>
            </div>
        </div>
    </div>
</div>