@{
    ViewBag.Title = "Application Proof Reading";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model Prp.Sln.ProofReadingModel
@section RenderCss{

    <style>

        td span {
            line-height: 18px;
        }


        #divprint {
            width: 100%;
            margin: 0 auto;
            margin-top: 15px;
            font-family: 'Lato', sans-serif !important;
        }

        .btn_print {
            padding: 10px 40px;
            background-color: #48a0d7;
            border-radius: 5px
        }

        .ank {
            cursor: pointer;
        }

        .spn-img-view {
            font-style: italic;
        }

        .spn-edit {
            float: right;
            margin-right: 5px;
            padding: 1px 6px;
            background: #fefefe;
            color: #8b8686;
        }

            .spn-edit:hover {
                background: #e9e0e0;
            }

        .imgView {
            width: 60px;
            height: 40px;
            border: 1px solid #ede9e9;
            width: 30px;
            height: 20px;
        }


        .text-center {
            text-align: center;
            text-align: -webkit-center;
            text-align: -moz-center;
        }

        .top_detail tr td:nth-child(2n) {
            border-bottom: 1px solid #000;
        }

        h3.heading {
            border-bottom: 1px solid;
            padding-bottom: 10px;
        }
    </style>
}
@section RenderScript{

    <script>

        var applicantId = 0, objApplicant = {}, imagesPath = "";

        $(document).ready(function () {

            applicantId = ConvertToInt($("#hfdApplicantId").val());

            $(document).on('click', '#ankSubmit', function () {

                var isConfirm = $("#chkConfirm").is(':checked');
                if (isConfirm) {

                    CallActionGet("/ApplicantProfile/UpdateApplicantStatus?applicantId=" + applicantId
                        + "&applicationStatus=8&condition=applicationStatus");
                    window.location = "/application-completed";
                }
                else {

                    alert("Please select agree information");
                }
            });
        });
    </script>
}
