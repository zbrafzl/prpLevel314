                                        <tbody>
                                            <tr>
                                                <th>Sr.#</th>
                                                <th>Type</th>
                                                <th>Institute</th>
                                                <th>From</th>
                                                <th>To</th>
                                                <th>Duration</th>
                                                <th>Is Continuous</th>
                                                <th>Province</th>
                                                <th>Type of Employment</th>
                                                <th>Certificate</th>
                                            </tr>
                                            @foreach (var exp in Model.listExprince)
                                            {
                                                sr = sr + 1;
                                                <tr>
                                                    <td> @sr</td>
                                                    <td>@exp.levelName</td>
                                                    <td>@exp.instituteName</td>
                                                    <td>@exp.startDated.ToString("dd MMM yyyy")</td>
                                                    <td>@exp.endDate.ToString("dd MMM yyyy")</td>
                                                    <td>@(exp.noOfMonth + " Month")</td>
