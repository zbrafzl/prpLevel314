@section RenderScript{

    <script>
var applicantId = 0, obj = {}, typeId = 1, leaveTypeId = 0;
        $(document).ready(function () {

            $('#fieldHistory .contentHistory').hide();
            $('#legendHistory').click(function () {
                $(this).parent().find('.contentHistory').slideToggle("slow");
            });

            applicantId = ConvertToInt($("#hfdApplicantId").val());


            






            var dateFormatDatePocker = 'dd/mm/yyyy';


            $("#txtDateFrom").datepicker({
                format: dateFormatDatePocker,
                autoclose: true,
                clearBtn: true,
                disableTouchKeyboard: true,
                todayHighlight: true,
                showOnFocus: true,
                changeYear: true,
                //setDate: new Date(),
                //startDate: new Date()
            });

            $("#txtDateTo").datepicker({
                format: dateFormatDatePocker,
                autoclose: true,
                clearBtn: true,
                disableTouchKeyboard: true,
                todayHighlight: true,
                showOnFocus: true,
                changeYear: true,
                //setDate: new Date(),
                //startDate: new Date()
            });



            $(document).on('click', '#ddlType', function () {

                $("#hfdTypeId").val($("#ddlType").val());

            });

            $(document).on('click', '#btnSave', function () {
                SaveApplicantAction();
            });

            $(document).keypress(function (event) {
                var keycode = (event.keyCode ? event.keyCode : event.which);
                if (keycode == '13') {
                    SaveApplicantAction();
                }
            });

        });

        function SaveApplicantAction() {

            if (ValidateFormJoining() == false) {

                console.log(obj);

                var resp = CallActionPost("/ApplicantAction/ExtensionSetupSave", obj);

                if (resp != null && resp.status == true) {
                    alert("Data saved successfully")
                    window.location = "/admin/extension-applicant-listing";
                }
                else {
                    if (resp.msg != null) {
                        alert(resp.msg);
                    }
                    else {
                        alert(resp.msg.msg);
                    }

                }
            }
            else {
                alert("Wrong");
            }
        }

        function epoch(date) {
            return Date.parse(date)
        }

        function ValidateFormJoining() {

            var isError = false, ctrl = "";

            obj = {};
            obj.applicantId = ConvertToInt($("#hfdApplicantId").val());
            obj.specialityJobId = ConvertToInt($("#hfdSpecialityJobId").val());
            obj.dateStart = ConvertToString($("#txtDateFrom").val());
            obj.dateEnd = ConvertToString($("#txtDateTo").val());
            obj.approvalBySupervisor = ConvertToInt($("#ddlApprovalBySupervisor").val());
            obj.statusId = 1;
            var dateToday = new Date(); // Mon Jun 08 2020 16:47:55 GMT+0800 (China Standard Time)
            var timestamp = epoch(dateToday);
            if ($("#flScanImage").val()) {

                var imgName = "Ext" + "_" + obj.applicantId + "_" + timestamp;
                obj.imageApplication = UploadImageAdmin("flScanImage", applicantId, imgName);
                $("#hfdScanImage").val(obj.imageApplication);
            }
            obj.imageApplication = ConvertToString($("#hfdScanImage").val());

            if ($("#flScanImagePER").val()) {
                var imgName = "ExtPER" + "_" + obj.applicantId + "_" + timestamp;
                obj.imagePER = UploadImageAdmin("flScanImagePER", applicantId, imgName);
                $("#hfdScanImagePER").val(obj.imagePER);
            }
            obj.imagePER = ConvertToString($("#hfdScanImagePER").val());

            if ($("#flScanImageNOC").val()) {
                var imgName = "ExtNOC" + "_" + obj.applicantId + "_" + timestamp;
                obj.imageNOC = UploadImageAdmin("flScanImageNOC", applicantId, imgName);
                $("#hfdScanImageNOC").val(obj.imageNOC);
            }
            obj.imageNOC = ConvertToString($("#hfdScanImageNOC").val());

            if ($("#flScanImagePMDC").val()) {
                var imgName = "ExtPMDC" + "_" + obj.applicantId + "_" + timestamp;
                obj.imagePMDC = UploadImageAdmin("flScanImagePMDC", applicantId, imgName);
                $("#hfdScanImagePMDC").val(obj.imagePMDC);
            }
            obj.imagePMDC = ConvertToString($("#hfdScanImagePMDC").val());

            if ($("#flScanImageExtensionOrder").val()) {
                var imgName = "ExtOrd" + "_" + obj.applicantId + "_" + timestamp;
                obj.imageExtensionOrder = UploadImageAdmin("flScanImageExtensionOrder", applicantId, imgName);
                $("#hfdScanImageExtensionOrder").val(obj.imageExtensionOrder);
            }
            obj.imageExtensionOrder = ConvertToString($("#hfdScanImageExtensionOrder").val());

            if ($("#flScanImageJoiningOrder").val()) {
                var imgName = "ExtJoin" + "_" + obj.applicantId + "_" + timestamp;
                obj.imageJoiningOrder = UploadImageAdmin("flScanImageJoiningOrder", applicantId, imgName);
                $("#hfdScanImageJoiningOrder").val(obj.imageJoiningOrder);
            }
            obj.imageJoiningOrder = ConvertToString($("#hfdScanImageJoiningOrder").val());

            if ($("#flScanImageDoc1").val()) {
	            var imgName = "ExtDoc1" + "_" + obj.applicantId + "_" + timestamp;
	            obj.imageDoc1 = UploadImageAdmin("flScanImageDoc1", applicantId, imgName);
	            $("#hfdScanImageDoc1").val(obj.imageDoc1);
            }
            obj.imageDoc1 = ConvertToString($("#hfdScanImageDoc1").val());

            if ($("#flScanImageDoc2").val()) {
                var imgName = "ExtDoc2" + "_" + obj.applicantId + "_" + timestamp;
                obj.imageDoc2 = UploadImageAdmin("flScanImageDoc2", applicantId, imgName);
                $("#hfdScanImageDoc2").val(obj.imageDoc2);
            }
            obj.imageDoc2 = ConvertToString($("#hfdScanImageDoc2").val());

            if (obj.dateStart == "") {

                $("#txtDateFrom").addClass("req-fld");
                isError = true;
                if (ctrl == "")
                    ctrl = "txtDateFrom";
            }

            if (obj.dateEnd == "") {

                $("#txtDateTo").addClass("req-fld");
                isError = true;
                if (ctrl == "")
                    ctrl = "txtDateTo";
            }

            if (obj.imageApplication == "") {

                $("#flScanImage").addClass("req-fld");
                isError = true;
                if (ctrl == "")
                    ctrl = "flScanImage";
            }

            if (obj.imagePER == "") {
                $("#flScanImagePER").addClass("req-fld");
                isError = true;
                if (ctrl == "")
                    ctrl = "flScanImagePER"
            }
            if (obj.imageNOC == "") {
                $("#flScanImageNOC").addClass("req-fld");
                isError = true;
                if (ctrl == "")
                    ctrl = "flScanImageNOC"
            }
            if (obj.imagePMDC == "") {
                $("#flScanImagePMDC").addClass("req-fld");
                isError = true;
                if (ctrl == "")
                    ctrl = "flScanImagePMDC"
            }
            if (obj.imageExtensionOrder == "") {
                $("#flScanImageExtensionOrder").addClass("req-fld");
                isError = true;
                if (ctrl == "")
                    ctrl = "flScanImageExtensionOrder"
            }
            if (obj.imageJoiningOrder == "") {
                $("#flScanImageJoiningOrder").addClass("req-fld");
                isError = true;
                if (ctrl == "")
                    ctrl = "flScanImageJoiningOrder"
            }
            
            console.log(obj);

            //ddlDoxTaken
            return isError;
        }</script>
}
